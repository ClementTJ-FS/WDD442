<% layout('./layouts/_default') %>
<div>
  <h1><%= it.choice.choiceText %></h1>
  <p><strong>ID: </strong><%= it.choice.id%></p>
  <p><strong>Created At: </strong><%= it.choice.createdAt %></p>
  <p><strong>Updated Last: </strong><%= it.choice.updatedAt %></p>
  <p>
    <a href="/choices/<%= it.choice.id %>/delete">Delete</a>
  </p>
</div>
<a href="/choices/">Back to All Choices.</a>
