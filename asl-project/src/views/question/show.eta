<% layout('./layouts/_default') %>
<div>
  <h1><%= it.question.questionText %></h1>
  <p><strong>ID: </strong><%= it.question.id%></p>
  <p><strong>Created At: </strong><%= it.question.createdAt %></p>
  <p><strong>Updated Last: </strong><%= it.question.updatedAt %></p>
  <p>
    <a href="/questions/<%= it.question.id %>/delete">Delete</a>
  </p>
</div>
<a href="/questions/">Back to Questions.</a>
